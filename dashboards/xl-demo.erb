<% content_for(:title) { "My XL Demo Dashboard" } %>
<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="allenvs" data-view="Number" data-title="All" style="background-color:#96bf48;"></div>
    </li>
    <li data-row="1" data-col="2" data-sizex="1" data-sizey="1">
      <div data-id="ACC" data-view="Meter" data-title="ACC" data-min="0" data-max="100"></div>
    </li>
    <li data-row="1" data-col="3" data-sizex="1" data-sizey="1">
      <div data-id="DEV" data-view="Meter" data-title="DEV" data-min="0" data-max="100"></div>
    </li>
    <li data-row="1" data-col="4" data-sizex="1" data-sizey="1">
      <div data-id="TEST" data-view="Meter" data-title="TEST" data-min="0" data-max="100"></div>
    </li>
    <li data-row="2" data-col="1" data-sizex="4" data-sizey="1">
      <div data-id="xlrdata" data-view="Table" data-image="/xl-release-logo.png" data-text="start"</div>
    </li>
    <li data-row="3" data-col="1" data-sizex="4" data-sizey="1">
      <div data-id="xlddata" data-view="Table" data-image="/xl-deploy-logo.png" data-text="start"</div>
    </li>
  </ul>
</div>
